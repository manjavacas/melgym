{
    "id": "sac",
    "tasks": [
        "test"
    ],
    "env": {
        "name": "hvac-v0",
        "params": {
            "max_deviation": 40,
            "check_done_time": 500,
            "control_horizon": 10,
            "render_mode": "distances"
        }
    },
    "algorithm": {
        "name": "SAC",
        "params": {
            "stats_window_size": 1
        },
        "train_params": {
            "total_timesteps": 20000,
            "eval_freq": 5000,
            "n_eval_episodes": 3
        }
    },
    "wrappers": [
        "norm_obs"
    ],
    "callbacks": [
        "EvalCallback",
        "TbMetricsCallback",
        "EpisodicDataCallback"
    ],
    "paths": {
        "tensorboard_dir": "./tensorboard/",
        "best_models_dir": "./best_models/",
        "ep_metrics_dir": "./ep_metrics/"
    }
}