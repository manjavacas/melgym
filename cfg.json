{
    "id": "td3",
    "tasks": [
        "train",
        "test"
    ],
    "env": {
        "name": "simple-v0",
        "params": {
            "max_deviation": 40,
            "check_done_time": 500,
            "control_horizon": 10,
            "render_mode": "distances"
        }
    },
    "algorithm": {
        "name": "TD3",
        "params": {
            "stats_window_size": 1,
            "learning_rate": 0.01
        },
        "train_params": {
            "total_timesteps": 40000,
            "eval_freq": 1000,
            "n_eval_episodes": 5
        }
    },
    "wrappers": [
        "norm_obs"
    ],
    "callbacks": [
        "EvalCallback",
        "TbMetricsCallback",
        "EpisodicDataCallback"
    ],
    "paths": {
        "tensorboard_dir": "./tensorboard/",
        "best_models_dir": "./best_models/",
        "ep_metrics_dir": "./ep_metrics/"
    }
}